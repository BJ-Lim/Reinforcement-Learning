    "import tensorflow as tf\n",
    "\n",
    "x = tf.constant([[1., 2., 3.]])\n",
    "w = tf.constant([[2.],[2.],[2.]])\n",
    "y = tf.matmul(x,w)\n",
    "print(x.get_shape())\n",
    "\n",
    "sess = tf.Session()\n",
    "sess.run(tf.global_variables_initializer())\n",
    "print(sess.run(y))\n"
   ]
